
<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <link rel="shortcut icon" href="/img/ico/favicon.ico">
    <link rel="apple-touch-icon" sizes="144x144" href="/img/ico/apple-touch-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/img/ico/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/img/ico/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" href="/img/ico/apple-touch-icon-57x57.png">

      <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    <!-- Bootstrap Core CSS -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/animate.css" rel="stylesheet">
    <link href="/css/plugins.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="/css/style.css" rel="stylesheet">



    <!-- Custom Fonts -->
    <link href="/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="/css/pe-icons.css" rel="stylesheet">


</head>

<body id="page-top" class="regular-navigation">


    <div class="master-wrapper">

        <div class="preloader">
            <div class="preloader-img">
            	<span class="loading-animation animate-flicker"><img src="/img/loading.GIF" alt="loading" /></span>
            </div>
        </div>

        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-fixed-top fadeInDown" data-wow-delay="0.2s">
            <div class="container-fluid">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-navigation">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="main-navigation">
                    <a class="navbar-brand smoothie" href="{{ path('home') }}">SNOW <span class="theme-accent-color">TRICKS</span></a>
                    <ul class="nav navbar-nav navbar-right collapse navbar-collapse navbar-ex1-collapse">
                        <li><a href="{{ path('home') }}" class="page-scroll">Accueil</a></li>
                        <li><a href="{{ path('trick_index') }}" class="page-scroll {% if current_menu is defined and current_menu == 'trick.list' %} active {% endif %}">Tricks</a></li>
                        <li><a href="{{ path('group_index') }}" class="page-scroll {% if current_menu is defined and current_menu == 'group.list' %} active {% endif %}">Groupes de tricks</a></li>
                        {% if app.user %}
                            <li><a href="#" class="page-scroll">Vous êtes : {{ app.user.pseudo }}</a></li>
                            <li><a href="{{ path('app_logout') }}" class="page-scroll">Se déconnecter</a></li>
                        {% else %}
                            <li><a href="{{ path('login') }}" class="page-scroll">Se connecter</a></li>

                        {% endif %}




                    </ul>

                </div>
                <!-- /.navbar-collapse -->         

            </div>
            <!-- /.container-fluid -->
        </nav>


        {% for message in app.flashes('success') %}
            <div class="alert alert-success "style="margin-top: 5%">
                {{ message }}
            </div>
        {% endfor %}
        <div class="container" style="margin-top: 5%; background-color: whitesmoke">
            {% block body %}{% endblock %}
        </div>

       
        <footer class="white-wrapper">
            <div class="container-fluid">
                <div class="row text-center">
                    <div class="col-md-12 wow fadeIn mb30" data-wow-delay="0.2s">
                        <span class="copyright">Copyright 2019. Designed by DISTINCTIVE THEMES</span>
                    </div>
                    <div class="col-md-12">
                        <ul class="list-inline social-links wow fadeIn" data-wow-delay="0.2s">
                            <li>
                                <a href="/#"><i class="fa fa-twitter"></i></a>
                            </li>
                            <li>
                                <a href="/#"><i class="fa fa-pinterest"></i></a>
                            </li>
                            <li>
                                <a href="/#"><i class="fa fa-dribbble"></i></a>
                            </li>
                            <li>
                                <a href="/#"><i class="fa fa-facebook"></i></a>
                            </li>
                            <li>
                                <a href="/#"><i class="fa fa-behance"></i></a>
                            </li>
                            <li>
                                <a href="/#"><i class="fa fa-linkedin"></i></a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>

        <div id="bottom-frame"></div>

        <a href="/#" id="back-to-top"><i class="fa fa-long-arrow-up"></i></a>

    </div>


    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        <script src="/js/jquery.js"></script>
        <script src="/js/bootstrap.min.js"></script>
        <script src="/js/plugins.js"></script>
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>


        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->

        <script type="text/javascript">
            $(document).ready(function(){
                'use strict';
                jQuery('#headerwrap').backstretch([
                    "img/bg/bg1.jpg",
                    "img/bg/bg2.jpg",
                    "img/bg/bg3.jpg",
                ], {duration: 8000, fade: 500});
            });

        </script>
    {% endblock %}
</body>

</html>
